  <ul class="all-questions">
    <% @questions.each do |question| %>
    <li class="question-link">
     	<%=question.votes.count %> Votes
     	<%= question.answers.count %> Answers 
     	<%= question.views %> views
     	<%= link_to question.title, question_path(question.id) %>
    </li>
    <% end %>
  </ul>


